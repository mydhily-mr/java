package day6;

class Lionapp
{
    public static void main(final String[] args) {
        final Lion l1 = new Lion();
        l1.name = "simba";
        l1.breed = "Asiatic";
        l1.gender = "male";
        System.out.println(l1.name);
        System.out.println(l1.breed);
        System.out.println(l1.gender);
        l1.eat();
        l1.sleep();
        l1.run();
    }
}