package day1;

public class HelloWorld
{
    public static void main(final String[] args) {
        System.out.println("Hello World...");
    }
}package day1;

public class Literals
{
    public static void main(final String[] args) {
        final float x = 1.2345678E7f;
        final float f = 1.2345678E7f;
        System.out.println(f);
    }
}