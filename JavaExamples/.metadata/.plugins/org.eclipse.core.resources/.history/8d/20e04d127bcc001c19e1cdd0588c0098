package day6;

class Lion {
    String name;
    String breed;
    String gender;

    Lion() {
    }

    void eat() {
        System.out.println("lion always eats non-veg");
    }

    void sleep() {
        System.out.println("lion sleeps 10 hr per day...!");
    }

    void run() {
        System.out.println("lion runs faster than human being");
    }
}